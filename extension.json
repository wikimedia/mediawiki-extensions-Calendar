{
	"name": "Calendar",
	"version": "1.1",
	"author": "Roland Unger",
	"url": "https://www.mediawiki.org/wiki/Extension:Calendar-Wikivoyage",
	"descriptionmsg": "calendar-desc",
	"type": "parserhook",
	"license-name": "GPL-2.0+",
	"ExtensionMessagesFiles": {
		"CalendarMagic": "Calendar.i18n.magic.php"
	},
	"Hooks": {
		"ParserFirstCallInit": [
			"CalendarHooks::setupParserHooks"
		]
	},
	"MessagesDirs": {
		"Calendar": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.calendar": {
			"position": "top",
			"styles": "ext.calendar.css",
			"localBasePath": "modules",
			"remoteExtPath": "Calendar/modules"
		}
	},
	"AutoloadClasses": {
		"CalendarTable": "CalendarTable.class.php",
		"CalendarHooks": "Calendar.hooks.php"
	},
	"manifest_version": 1
}
